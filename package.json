{
	"name": "diploma-work",
	"version": "0.1.0",
	"author": "OskV2",
	"description": "Typescript, Electron, React, SCSS, Hot-Reloading",
	"homepage": ".",
	"main": "build/electron/main.js",
	"private": true,
	"devDependencies": {
		"@typescript-eslint/parser": "^6.8.0",
		"concurrently": "^8.2.2",
		"cross-env": "^7.0.3",
		"electron": "^27.0.2",
		"electron-builder": "^24.6.4",
		"wait-on": "^7.0.1"
	},
	"dependencies": {
		"@types/jest": "29.5.6",
		"@types/node": "20.8.7",
		"@types/node-sass": "^4.11.6",
		"@types/react": "18.2.31",
		"@types/react-dom": "18.2.14",
		"electron-reloader": "^1.2.3",
		"node-sass": "^9.0.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-scripts": "5.0.1",
		"typescript": "5.2.2"
	},
	"scripts": {
		"dev": "concurrently \"cross-env BROWSER=none react-scripts start\" \"wait-on http://localhost:3000 && tsc -p ./electron -w\" \"wait-on http://localhost:3000 && tsc -p ./electron && electron .\"",
		"dev-nomainreload": "concurrently \"cross-env BROWSER=none react-scripts start\" \"wait-on http://localhost:3000 && tsc -p ./electron && electron .\"",
		"build": "react-scripts build && tsc -p electron && electron-builder",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"postinstall": "electron-builder install-app-deps"
	},
	"build": {
		"extends": null,
		"files": [
			"build/**/*"
		],
		"directories": {
			"buildResources": "assets"
		}
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
